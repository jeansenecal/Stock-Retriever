<%- include('partials/head') -%>
<body>
    
  <%- include('partials/nav') -%>
    <section class="p-20 bg-secondary-content h-screen">
        <div class="overflow-x-auto w-full">
            <table class="table w-full table-zebra">
              <!-- head -->
              <thead>
                <tr>
                  <th>Stock Symbol</th>
                  <th>Hype</th>
                  <th>Date Bought</th>
                  <th>Bought Price</th>
                  <th>Date Sold</th>
                  <th>Sold Price</th>
                  <th>Return $</th>
                  <th>Return %</th>
                </tr>
              </thead>
              <tbody>
                <% for(var i=0; i<soldStocks.length; i++) { %>
                  <tr>
                      <td><%- soldStocks[i].symbol %></td>
                      <td><%- soldStocks[i].hype %></td>
                      <td><%- soldStocks[i].dateBought.toDateString() %></td>
                      <td><%- soldStocks[i].boughtPrice %> $</td>
                      <td><%- soldStocks[i].dateSold.toDateString() %></td>
                      <td><%- soldStocks[i].soldPrice %> $</td>
                      <td><%- soldStocks[i].return %> $</td>
                      <td><%- soldStocks[i].returnPercentage %>%</td>
                  </tr>
                <% } %>
            </table>
          </div>
    </section>
  <%- include('partials/footer') -%>
</body>
</html>